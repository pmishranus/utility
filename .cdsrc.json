{ "build": {
    "target": "gen",
    "tasks": [
      {
        "for": "hana",
        "src": "db",
        "options": {
          "model": [
            "db",
            "srv",
            "eclaims",
            "cws"
          ]
        }
      },
      {
        "for": "nodejs",
        "src": "srv",
        "dest": "srv",
        "options": {
          "model": [
            "db",
            "srv",
            "eclaims",
            "cws"
            
          ]
        }
      },
      {
        "for": "nodejs",
        "src": "eclaims",
        "dest": "eclaims",
        "options": {
          "model": [
            "db",
            "srv",
            "eclaims",
            "cws"
          ]
        }
      },
      {
        "for": "nodejs",
        "src": "cws",
        "dest": "cws",
        "options": {
          "model": [
            "db",
            "srv",
            "eclaims",
            "cws"
          ]
        }
      }
    ]
  },
  "requires": {
    "db": {
      "kind": "hana-cloud",
      "model": [
        "db",
        "srv",
        "eclaims",
        "cws"
      ]
    }
  }

}